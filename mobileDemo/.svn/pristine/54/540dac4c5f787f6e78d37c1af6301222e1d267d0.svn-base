      function Noti(prorn, toptit) {
          var arry = [];
          var y;
          var x = 0;
          var b = 0;
          $(prorn).each(function (i) {
              y = parseInt($(prorn).eq(i).html());
              arry = arry;
              arry.push(y);
              x = arry[i];
              b += x;
              //		console.log(b);
          });
          $(toptit).html(b);
      }
