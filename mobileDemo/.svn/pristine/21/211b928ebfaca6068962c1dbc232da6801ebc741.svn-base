              softInputMode: 'resize',
              pageParam: {id: id, qf: 'question-detail', memberId: memberId, taskId: taskId, courseId: courseId},
              vScrollBarEnabled: false
          });
      }
      apiready = function () {
          api.addEventListener({
              name: 'my-question-lx'
          }, function (ret) {
              getData(1);
          });
          //视频新建问答刷新页面
          api.addEventListener({
              name: 'create-question-f'
          }, function (ret) {
              getData(1);
          });
      
          getData(1);
      };
    </script>
  </body>
</html>