html,body{width:100%;height:100%;-webkit-user-select:none;-webkit-text-size-adjust:none; }
h1,h2,h3,h4,h5,h6{font-size: 16px;font-weight: normal;}
ul{list-style: none;padding: 0;margin:0;}
*{padding:0;margin:0; font-family:"STXihei","黑体","幼圆",sans-serif;}
img{vertical-align: top;border:none;}
a{text-decoration: none;display: inline-block;*display: inline;color: #888994;}
textarea,input{outline:none;border: none;}/*背景图部分是为了解决ie7.8下定位的输入层不能控制层级的bug*/
input{border:0;outline: none;-webkit-appearance: none;background:none;}
textarea{border: none;outline: none;-webkit-appearance: none;}
input,button,textarea {
	border:0;
	margin:0;
	padding:0;
	font-size:1em;
	line-height:1em;
	/*-webkit-appearance:none;*/
	background-color:rgba(0, 0, 0, 0);
}
.clearfix:after{display: block;content: "";clear:both;}
.clear{clear: both;}

.hide{overflow: hidden;}
.none{display: none;}

.right{ float: right;}
.left{ float: left;}
#header{position:relative;}

