.down-progress
    width .6rem
    height .6rem
    color #999
    &:before
        content ''
        position absolute
        z-index 3
        width 100%
        height 100%
        top 0
        left 0
        /*background rgba(200,30,0,.3)*/
    &[status='ing'],.ing,&[type='1']
        i:before
            content '\e95f'
    &[status='end'],&.end,&[type='4']

        svg
            circle
              stroke $color_green
        i
            color $color_green
            font-size .4rem !important
            &:before
                content '\e913'

    &[status='wait'],&.wait,&[type='2']
        i:before
            content '\e909'
    i
        top 50%
        left 50%
        transform translate(-50%,-50%)
        -webkit-transform translate(-50%,-50%)
        line-height .5rem
        font-size .5rem !important

.down-progress,.audio-progress
    .val
        display none
    svg
        circle
            -webkit-transition: stroke-dasharray .3s
            transition: stroke-dasharray .3s
            stroke #ccc
            &:last-child
                stroke $color_green

.down-progress svg circle
    stroke-width 1

.audio-progress svg circle
    stroke-width 2

