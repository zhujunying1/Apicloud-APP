/**圆环进度条**/
circle
    -webkit-transition: stroke-dasharray .5s .8s
    transition: stroke-dasharray .5s .8s

cir_w = 2rem
.circle-progress,.down-progress,.audio-progress
    width cir_w
    height cir_w
    position relative
    display inline-block
    vertical-align middle
    text-align center

    .text,.val,i
        position absolute
        left 0
        width 100%
    .text
        top 30%
    .val
        bottom 30%
        font-size .4rem
        &:after
            content '%'
    svg
        margin auto
        circle
            stroke-width 6
            fill none
            stroke rgba(255,255,255,.3)
            &:last-child
                stroke rgba(255,255,255,1)
                stroke-dasharray 1 1000
    i
        top 50%
        left 50%
        -webkit-transform translate(-50%,-50%)

.ios9,.android5
    .circle-progress,.down-progress,.audio-progress
        svg
            position absolute
            top 50%
            left 50%
            -webkit-transform translate(-50%,-50%) rotate(-90deg)
.down_speed
    position absolute
    color #999
    top .4rem
    right .9rem
    &:after
        content "kb/s"
#chaList
  .down_speed
    position absolute
    color #999
    top .4rem
