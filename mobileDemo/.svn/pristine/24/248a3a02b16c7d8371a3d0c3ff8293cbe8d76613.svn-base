  padding: 0.2rem 0;
}
.dropdown-menu:before {
  content: '';
  position: absolute;
  border: 0.2rem solid transparent;
  border-bottom-color: rgba(0,0,0,0.8);
  top: -0.395rem;
  left: 50%;
  margin-left: -0.15rem;
}
.dropdown-menu li {
  padding: 0.2rem 0.6rem;
}
.dropdown-menu li:not(:last-child) {
  border-bottom: 1px solid #fff;
}
