.progress-round .progress-round-canvas {
  width: 1rem;
  height: 1rem;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}

circle {
  -webkit-transition: stroke-dasharray 0.5s 0.8s;
  transition: stroke-dasharray 0.5s 0.8s;
}
.circle-progress,
.down-progress,
.audio-progress {
  width: 2rem;
  height: 2rem;
  position: relative;
  display: inline-block;
  vertical-align: middle;
  text-align: center;
}
.circle-progress .text,
.down-progress .text,
.audio-progress .text,
.circle-progress .val,
.down-progress .val,
.audio-progress .val,
.circle-progress i,
.down-progress i,
.audio-progress i {
  position: absolute;
  left: 0;
  width: 100%;
}
.circle-progress .text,
.down-progress .text,
.audio-progress .text {
  top: 30%;
}
.circle-progress .val,
.down-progress .val,
.audio-progress .val {
  bottom: 30%;
  font-size: 0.4rem;
}
.circle-progress .val:after,
.down-progress .val:after,
.audio-progress .val:after {
  content: '%';
}
.circle-progress svg,
.down-progress svg,
.audio-progress svg {
  margin: auto;
}
.circle-progress svg circle,
.down-progress svg circle,
.audio-progress svg circle {
  stroke-width: 6;
  fill: none;
  stroke: rgba(255,255,255,0.3);
}
.circle-progress svg circle:last-child,
.down-progress svg circle:last-child,
.audio-progress svg circle:last-child {
  stroke: #fff;
  stroke-dasharray: 1 1000;
}
.circle-progress i,
.down-progress i,
.audio-progress i {
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
}
.ios9 .circle-progress svg,
.android5 .circle-progress svg,
.ios9 .down-progress svg,
.android5 .down-progress svg,
.ios9 .audio-progress svg,
.android5 .audio-progress svg {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%) rotate(-90deg);
}
.down_speed {
  position: absolute;
  color: #999;
  top: 0.4rem;
  right: 0.9rem;
}
.down_speed:after {
  content: "";
}
#chaList .down_speed {
  position: absolute;
  color: #999;
  top: 0.4rem;
  right: 1rem;
}
#chaList .down_speed:after {
  content: "";
}
ul.download li {
  padding-right: 1.3rem;
}
ul.download .down-progress {
  position: absolute;
  top: 0.17rem;
  right: 0.3rem;
}
.down-progress {
  width: 0.6rem;
  height: 0.6rem;
  color: #999;
}
.down-progress:before {
  content: '';
  position: absolute;
  z-index: 3;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
/*background rgba(200,30,0,.3)*/
}
.down-progress i{
	font-size: 0.4rem;
}
.down-progress[status='ing'] i:before,
.down-progress .ing i:before,
.down-progress[type='1'] i:before {
  content: '\e95f';
}
.down-progress[status='end'] svg circle,
.down-progress.end svg circle,
.down-progress[type='4'] svg circle {
  stroke: #00a085;
}
.down-progress[status='end'] i,
.down-progress.end i,
.down-progress[type='4'] i {
  color: #00a085;
  font-size: 0.4rem !important;
}
.down-progress[status='end'] i:before,
.down-progress.end i:before,
.down-progress[type='4'] i:before {
  content: '\e913';
}
.down-progress[status='wait'] i:before,
.down-progress.wait i:before,
.down-progress[type='2'] i:before,
 .down-progress[type='5'] i:before {
  content: '\e909';
}

.down-progress i {
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  -webkit-transform: translate(-50%, -50%);
  line-height: 0.5rem;
  font-size: 0.5rem !important;
}
.down-progress .val,
.audio-progress .val {
  display: none;
}
.down-progress svg circle,
.audio-progress svg circle {
  -webkit-transition: stroke-dasharray 0.3s;
  transition: stroke-dasharray 0.3s;
  stroke: #ccc;
}
.down-progress svg circle:last-child,
.audio-progress svg circle:last-child {
  stroke: #00a085;
}
.down-progress svg circle {
  stroke-width: 1;
}
.audio-progress svg circle {
  stroke-width: 2;
}
