/*消息中心*/
@media screen and (max-width: 321px) and (min-width: 320px) {
  .note-font {
    line-height: 1.26;
  }
}
@-webkit-keyframes move {
  0% {
    width: 0.1rem;
  }
  25% {
    width: 0.12rem;
  }
  50% {
    width: 0.25rem;
  }
}
.section-note {
  position: relative;
}
.section-note-content {
  position: relative;
  width: 100%;
  background-color: #fff;
  border-bottom: 1px solid #f1f1f1;
  padding-bottom: 0.3rem;
  overflow: hidden;
  font-size: 0.24rem;
}
.section-note-content .section-note-top span {
  max-height: 1.2rem;
  overflow: hidden;
}
.section-note-content .section-note-top .note-font p {
  overflow: hidden;
  text-overflow: ellipsis;
}
.section-note-content .section-note-top2 .note-font p {
  overflow: hidden;
  text-overflow: clip;
  word-break: break-all;
}
/*章节可重用样式*/
.section-note-top {
  width: 91.6%;
  display: table;
  font-size: 0.24rem;
  margin: 0 auto;
}
.section-note-message {
  width: 91.6%;
}
.section-note-message span {
  display: inline;
  /* vertical-align: middle; */
}
.section-note-message span:nth-child(2) {
  position: absolute;
  display: block;
  top: 0;
  right: 0;
  text-align: right;
}
.section-note-message span:nth-child(2) b {
  margin-left: 0.1rem;
}
.section-note-message span:nth-child(2) i {
  margin-right: 0.06rem;
}
.section-note-message span:nth-child(2) img {
  width: 20%;
}
.section-note-message .video-link-img {
  height: 0.28rem;
  width: 0.28rem;
}
.section-note-message img {
  vertical-align: middle;
}
.section-note-message b {
  vertical-align: middle;
  font-style: normal;
  font-weight: normal;
  color: #9f9f9f;
  font-size: 0.22rem;
}
.section-note-message b:nth-child(2) {
  /*margin-left: -4%;*/
  margin-left: 2%;
}
.section-note-message b:nth-child(3) {
  margin-left: 2%;
}
.section-note-message b:nth-child(4) {
  margin-left: 3%;
}
.section-note-message b:nth-child(5) {
  margin-left: 3%;
}
.section-note-message .icon-replys {
  color: #999;
  font-size: 0.35rem;
  vertical-align: middle;
}
.section-note-message .icon-play-o {
  color: #999;
  font-size: 0.3rem;
  vertical-align: sub;
}
.note-content-Img {
  clear: both;
}
.note-content-Img .note-img {
  width: 25.7%;
  height: 1.2rem;
  overflow: hidden;
  float: left;
  margin-right: 2%;
}
.note-content-Img .note-img img {
  max-width: 100%;
}
.note-content-Img .note-img ~ .note-font {
  width: 70.8%;
  float: left;
}
.note-content-Img .note-font {
  max-height: 1.2rem;
  line-height: 0.4rem;
  overflow: hidden;
  text-overflow: clip;
  word-break: break-all;
}
.note-content-Img .note-font p {
  color: #666;
  font-size: 0.24rem;
  padding-top: 0.1rem;
  line-height: 0.37rem;
}
.note-content-Img .section-note-top {
  position: relative;
  margin-top: 0.3rem;
}
.note-content-Img .section-note-top img {
  margin-top: 0.1rem;
  height: 1.15rem;
  width: 100%;
}
.note-content-Img .section-note-message {
  position: relative;
  margin-top: 0.3rem;
  height: 0.3rem;
  left: 4.2%;
  font-size: 0.22rem;
}
.note-content-noneImg .section-note-message {
  position: relative;
  margin-top: 0.3rem;
  left: 4.2%;
}
.note-content-noneImg .section-note-message b:nth-child(3) {
  margin-left: 4%;
}
.note-content-noneImg .section-note-top {
  position: relative;
  font-size: 0.24rem;
  margin-top: 0.3rem;
}
.note-content-noneImg .section-note-top p {
  font-size: 0.24rem;
}
.star,
.good {
  height: 0.25rem;
  width: 0.3rem !important;
  margin-left: 0.3rem;
}
.course-question .section-note-message img {
