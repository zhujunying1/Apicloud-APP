/*课程-讨论*/
.Advertisement {
  position: relative;
  width: 100%;
  height: 1.6rem;
  border-bottom: 1px solid #e6e6e6;
}
.closeAdvertisement {
  position: absolute;
  width: 0.47rem;
  top: 0.16rem;
  right: 0.16rem;
}
.AdvertisementPic {
  width: 100%;
  height: 1.6rem;
}
.toTopList {
  background: #fff;
}
.toTopList li {
  height: 0.9rem;
  border-bottom: 1px solid #e6e6e6;
  position: relative;
  padding: 0 0.2rem 0 1rem;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 0.28rem;
  color: #636363;
  line-height: 0.9rem;
}
.toTopList li .toTop {
  font-size: 0.22rem;
  color: #00a185;
  border: 1px solid #00a185;
  border-radius: 2px;
  padding: 0 0.04rem;
  position: absolute;
  top: 0.25rem;
  left: 0.3rem;
  height: 0.36rem;
  line-height: 0.34rem;
}
.my-talk {
  width: 100%;
  background-color: #f1f1f1;
  margin-bottom: 0.88rem;
}
.talk-content {
  font-size: inherit;
}
.my-talk-tab {
  height: 0.86rem;
  display: flex;
  width: 100%;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: flex-start;
  font-size: 0.3rem;
  border: 1px solid #d5d5d5;
}
.my-talk-tab > div {
  flex: 1;
  width: 50%;
  height: 100%;
  line-height: 100%;
  position: relative;
  box-sizing: border-box;
}
.my-talk-tab > div span {
  width: 100%;
  height: 100%;
  line-height: 250%;
  text-align: center;
}
.talk-tab-selected {
  border-bottom: 3px solid #00a185;
}
.talk-tab-selected span {
  color: #22a891;
}
