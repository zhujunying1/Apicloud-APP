/*课程-学习进度-更多*/
.courseStudyMore {
  width: 100%;
  height: 0.88rem;
  text-align: center;
  line-height: 0.88rem;
  font-size: 0.32rem;
  color: #fff;
  border-top: 1px solid #e4e4e4;
  background: #00a185;
  opacity: 0.001;
  transition: opacity 0.2s;
}
.courseStudyMore.show {
