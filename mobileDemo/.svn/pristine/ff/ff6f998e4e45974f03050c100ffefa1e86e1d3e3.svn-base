/*苏男男添加笔记详情底部*/
.fotter {
  position: fixed;
  bottom: 0;
  height: 0.95rem;
  width: 100%;
  border-top: 1px solid #d5d5d5;
  background: #fff;
  overflow: hidden;
}
.fotter div {
  width: 50%;
  height: 100%;
  padding-top: 0.25rem;
  border-right: 1px solid #d5d5d5;
}
.fotter div:first-child {
  position: absolute;
  left: 0;
}
.fotter div:last-child {
  position: absolute;
  right: 0;
}
.fotter div .icon {
  float: left;
  margin-left: 1rem;
  font-size: 0.4rem;
  color: #00a185;
}
.fotter div span {
  display: block;
  height: 0.4rem;
  padding-left: 0.5rem;
  font-size: 0.32rem;
  color: #00a185;
  margin-left: 1rem;
}
/**通用图片组**/
.pic-group {
  margin: 0.3rem 0 0.1rem 0;
  font-size: 0;
  white-space: nowrap;
  position: relative;
  overflow: hidden;
  width: 100%;
}
.pic-group li {
  display: inline-block;
  width: 32%;
  margin: 0 1%;
  height: 2rem;
  line-height: 2rem;
  text-align: center;
  background: #eee no-repeat center;
  background-size: cover;
}
.pic-group b {
  position: absolute;
  z-index: 5;
  top: 1.6rem;
  background-color: rgba(0,0,0,0.5);
  color: #fff;
  height: 0.4rem;
  width: 1rem;
  text-align: center;
  line-height: 0.4rem;
  font-size: 0.22rem;
  margin-left: -1%;
  transform: translate(-100%, 0);
  -webkit-transform: translate(-100%, 0);
}
/*********************** Chpter notes*/
.chapter {
  position: relative;
}
.chapnote1 {
  font-size: 0.24rem;
