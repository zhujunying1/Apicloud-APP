extends ../layout/layout
block head
    link(rel='stylesheet', href='../css/swiper.min.css')
    link(rel='stylesheet', href='../css/user-center.css')
    link(rel='stylesheet', href='../css/about-video.css')
block body
    // 题目内容56576457645
    .swiper-container
        .swiper-wrapper#pdf_content
        // Add Arrows
        .swiper-button-next
        .swiper-button-prev
    .qesition_complete.none
        .ce_box 阅读结束
        .ce_box2

        .ce_box3
        .ce_btn.cl
            .again_btn.com_btn(onclick='again_task()',tapmode) 再看一遍
            .next_btn.com_btn(onclick='next_task()',tapmode) 下个任务

    //测试题
    script(id='pdf_tpl',type='text/x-dot-template')
        |{{~it.pdfPic:value:key}}
        .course-content.swiper-slide
            img(style='width:100%;height:auto;margin-top:-2rem;',src='{{=common_url+value}}')
        |{{~}}
block scripts
    script(type='text/javascript', src='../script/jquery.min.js')
    script(type='text/javascript', src='../script/swiper.js')
    script(type='text/javascript', src='../script/pagejs/course-test-pdf.js')